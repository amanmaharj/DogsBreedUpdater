<!-- route url had to match with the form url to continue -->
<form method="POST" action="/dogs/<%=dogIndex%>/?_method=PUT" >
    <label for="name">Name</label>
    <input type="text" name="name" id="name" value="<%= myDog.name%>">
    <label for="type">Type</label>
    <input type="text" name="type" id="type" value="<%= myDog.type%>">
    <input type="submit" value="Enter">

</form>